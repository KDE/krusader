{
    "KDE-KIO-Protocols": {
        "krarc": {
            "Class": ":local",
            "Icon": "package-x-generic",
            "archiveMimetype": [
                "application/x-7z",
                "application/x-7z-compressed",
                "application/x-ace",
                "application/x-ace-compressed",
                "application/x-arj",
                "application/x-arj-compressed",
                "application/x-cpio",
                "application/x-lha",
                "application/x-lha-compressed",
                "application/x-rar",
                "application/x-rar-compressed",
                "application/vnd.rar",
                "application/zip",
                "application/x-zip",
                "application/x-zip-compressed"
            ],
            "copyToFile": true,
            "deleting": true,
            "input": "filesystem",
            "linking": false,
            "listing": [
                "Name",
                "Type",
                "Size",
                "Date",
                "Access",
                "Owner",
                "Group",
                "Link"
            ],
            "makedir": true,
            "moving": false,
            "output": "filesystem",
            "protocol": "krarc",
            "reading": true,
            "source": true,
            "writing": true
        }
    }
}

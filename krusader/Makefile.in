krusader_meta_unload.cpp:  krslots.moc krusader.moc krusaderview.moc
	@echo 'creating krusader_meta_unload.cpp'
	@-rm -f krusader_meta_unload.cpp
	@if test ${kde_qtver} = 2; then \
		echo 'static const char * _metalist_krusader[] = {' > krusader_meta_unload.cpp ;\
		cat  krslots.moc krusader.moc krusaderview.moc | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> krusader_meta_unload.cpp ;\
		echo '0};' >> krusader_meta_unload.cpp ;\
		echo '#include <kunload.h>' >> krusader_meta_unload.cpp ;\
		echo '_UNLOAD(krusader)' >> krusader_meta_unload.cpp ;\
	else echo > krusader_meta_unload.cpp; fi



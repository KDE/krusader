krusader.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp $(srcdir)/krusaderview.cpp $(srcdir)/krusader.cpp $(srcdir)/krslots.cpp $(srcdir)/kicons.cpp $(srcdir)/krusader_meta_unload.cpp   krslots.moc krusader.moc krusaderview.moc 
	@echo 'creating krusader.all_cpp.cpp ...'; \
	rm -f krusader.all_cpp.files krusader.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> krusader.all_cpp.final; \
	for file in main.cpp krusaderview.cpp krusader.cpp krslots.cpp kicons.cpp krusader_meta_unload.cpp ; do \
	  echo "#include \"$$file\"" >> krusader.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> krusader.all_cpp.final; \
	done; \
	cat krusader.all_cpp.final krusader.all_cpp.files  > krusader.all_cpp.cpp; \
	rm -f krusader.all_cpp.final krusader.all_cpp.files

